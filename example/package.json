{
  "name": "example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:sprite": "ikode --out sprite.js --name pngsprite -d icons/16x16 icons/large",
    "build:sprite:no-prompt": "ikode --out sprite.js --name pngsprite -y -d icons/16x16 icons/large",
	"build:sprite-json": "ikode --json --out sprite.json -d icons/16x16 icons/large",
	"build:sprite-json:no-prompt": "ikode --json --out sprite.json -y -d icons/16x16 icons/large",
	"build:sprite-css": "ikode --css --out sprite.css --important --ext extensions.json -d icons/16x16 icons/large",
	"build:sprite-css:no-prompt": "ikode --css --out sprite.css --important --ext extensions.json -y -d icons/16x16 icons/large",
	"build": "yarn build:sprite && yarn build:sprite-json && yarn build:sprite-css",
	"build:no-prompt": "yarn build:sprite:no-prompt && yarn build:sprite-json:no-prompt && yarn build:sprite-css:no-prompt"
  },
  "devDependencies": {}
}
